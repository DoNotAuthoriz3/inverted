package inverted.holdings.code.p005;

public class forXample
{	
	public static void main(String[] args)
	{
		// TODO Auto-generated method stub
		// 1, 1, 2, 3, 5, 8, 13, 21, ...
		int x = 1, y = 1;
		
		do 
		{

			
			System.out.print("" + x + ", ");
			x = y + x;
			y = x - y;
			try
			{
				Thread.sleep(1000);
			} catch (InterruptedException e)
			{
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			
		} while (true);
		
	}
}
