		// 1, 1, 2, 3, 5, 8, 13, 21, ...
		int x = 1, y = 0;
		
		for(int count = 1; count < 15; count++)
		{
			System.out.print("" + x + ", ");
			
			x = y + x;
			y = x - y;
			count = count + 1;
		}
	}
}










