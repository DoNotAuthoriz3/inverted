#include <iostream>
using namespace std;

struct newStruct
{
	int test;
	string teste;
};

int main(int argc, const char* argv[])
{
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!

	int x = 0, y = 0;
	int* z;
	newStruct instance;

	z = (int*) calloc(20, sizeof(int));

	for (int i=0; i<10; i++)
	{
		x = 1;
		y = y + x;
		z[y] = y * y;
		instance.test = z[y] + instance.test;
	}

	for (int i = 0; i < 20; i++)
		cout << "z[i] = " << z[i] << endl;

	cout << "instance.test is " << instance.test << endl;

	return 0;
}

